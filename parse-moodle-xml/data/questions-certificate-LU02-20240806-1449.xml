    <answer fraction="0" format="html">
      <text><![CDATA[<p>No, because chlorine is not listed in one of the three Schedules<br></p>]]></text>
      <feedback format="html">
        <text></text>
      </feedback>
    </answer>
    <answer fraction="50" format="html">
